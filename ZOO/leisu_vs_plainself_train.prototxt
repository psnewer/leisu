name: "leisu"

layer {
   type: "HDF5Data"
   top: "home_res" # same name as given in create_dataset!
   top: "away_res"
   top: "labels"
   hdf5_data_param {
     source: "/Users/miller/Documents/workspace/leisu/res/extract/total/VS_PLAINSELF_EXTRACTOR_train.txt" # do not give the h5 files directly, but the list.
     batch_size: 30
   }
   include { phase:TRAIN }
 }

layer {
   type: "HDF5Data"
   top: "home_res" # same name as given in create_dataset!
   top: "away_res"
   top: "labels"
   hdf5_data_param {
     source: "/Users/miller/Documents/workspace/leisu/res/extract/total/VS_PLAINSELF_EXTRACTOR_test.txt" # do not give the h5 files directly, but the list.
     batch_size: 10
   }
   include { phase:TEST }
}

layer {
    name: "Dense_home1"
    type: "InnerProduct"
    bottom: "home_res"
    top: "Dense_home1"
    param {
        lr_mult: 1
        decay_mult: 1
    }
    param {
        lr_mult: 2
        decay_mult: 0
    }
    inner_product_param {
        num_output: 128
        weight_filler {
            type: "xavier"
        }
        bias_filler {
            type: "constant"
            value: 0
        }
    }
}

layer {
  name: "home_relu1"
  bottom: "Dense_home1"
  top: "home_relu1"
  type: "ReLU"
  relu_param {
    negative_slope: 0.01
  }
}

layer {
    name: "Dense_home2"
    type: "InnerProduct"
    bottom: "home_relu1"
    top: "Dense_home2"
