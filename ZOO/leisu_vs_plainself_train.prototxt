################################################################################

layer {
  name: "concate_feature"
  type: "Concat"
  bottom: "home_relu4"
  bottom: "away_relu4"
  top: "concate_feature"
  concat_param {
    axis: 1
  }
}

layer {
  name: "ip1_cls"
  type: "InnerProduct"
  bottom: "concate_feature"
  top: "ip1_cls"
  param {
    lr_mult: 1
    decay_mult: 1
  } 
  param {
    lr_mult: 2
    decay_mult: 0
  } 
  inner_product_param {
    num_output: 128
    weight_filler {
      type: "gaussian"
      std: 0.001
    } 
    bias_filler {
      type: "constant"
      value: 0
    } 
  } 
} 


layer {
  name: "relu_ip1_cls"
  type: "ReLU"
  bottom: "ip1_cls"
  top: "ActivationRelu_ip1_cls"
}

layer {
  name: "ip2_cls"
  type: "InnerProduct"
  bottom: "ActivationRelu_ip1_cls"
  top: "ip2_cls"
  param {
    lr_mult: 1
    decay_mult: 1
  } 
  param {
    lr_mult: 2
    decay_mult: 0
  } 
  inner_product_param {
    num_output: 64
    weight_filler {
      type: "gaussian"
      std: 0.001
    } 
    bias_filler {
      type: "constant"
      value: 0
    } 
  } 
} 

layer {
   name: "relu_ip2_cls"
   type: "ReLU"
   bottom: "ip2_cls"
   top: "ActivationRelu_ip2_cls"
}

layer {
   name: "ip3_cls"
   type: "InnerProduct"
   bottom: "ActivationRelu_ip2_cls"
   top: "ip3_cls"
   param {
     lr_mult: 1
     decay_mult: 1
   } 
   param {
     lr_mult: 2
     decay_mult: 0
   } 
   inner_product_param {
     num_output: 3
     weight_filler {
       type: "gaussian"
       std: 0.001
     } 
     bias_filler {
       type: "constant"
       value: 0
     } 
   } 
}

layer {
  name: "accuracy"
  type: "Accuracy"
  bottom: "ip3_cls"
  bottom: "labels"
  top: "accuracy"
  top: "per_correct"
  top: "per_count"
  accuracy_param {
	ignore_label: 1
  }
}

layer {
   name: "loss"
   type: "SoftmaxWithLoss"
   bottom: "ip3_cls"
   bottom: "labels"
   top: "loss"
   loss_param {
     ignore_label: 1	
   }
}

